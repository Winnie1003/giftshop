 /*楼梯导航css样式*/
#LoutiNav{border:1px solid #666;position:fixed;top:150px;left:15px;display:none;}
#LoutiNav ul li{width:35px;height:35px;border-bottom:1px dotted #ddd;font-size:14px;text-align:center;line-height:35px;
                position:relative;cursor:pointer;}
#LoutiNav ul li.last{background:#C00;color:#fff;}
#LoutiNav ul li span{width:35px;height:35px;background:#C00;color:#fff;display:block;font-size:12px;position:absolute;left:0;
                    top:0;display:none;}
#LoutiNav ul li.hover span{display:block;}
#LoutiNav ul li span.active{background:#fff;color:#C00;display:block;}
#LoutiNav ul li.hover span.active{background:#C00;color:#fff;display:block;font-size:12px;}